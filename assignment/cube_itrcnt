import random 

guess = 2.0
cube = 27
increment = 0.1
elipsion = 0.1
cnt = 0

counter = [5,10,15,20]

for i in counter :
    while(i):
        if abs(guess**3 - cube) >= elipsion:
            guess = guess + increment
            print(guess)
                
if abs(guess**3 - cube) >= elipsion:
    print("faild on the cude root of" , cube)
else:
    print("\n")
    print(  cnt, ")" ,  guess , "is the cude root of" , cube)

    
    
    
